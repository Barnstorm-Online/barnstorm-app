{
  "name": "barnstorm-online",
  "version": "1.0.0",
  "description": "Get Active",
  "license": "MIT",
  "productName": "Barnstorm Online",
  "cordovaId": "online.barnstorm.dashboard",
  "author": "Michael J Feher <github@phearzero.com>",
  "scripts": {
    "lint": "eslint --ext .js,.vue src",
    "test": "cross-env NODE_ENV=dev jest",
    "test:unit": "cross-env NODE_ENV=dev jest",
    "test:unit:watch": "cross-env NODE_ENV=dev jest --watch",
    "test:e2e": "cypress open",
    "test:e2e:CI": "cypress run",
    "test:lighthouse": "lighthouse http://localhost:8080 view --output-path=./test/lighthouse/reports/index.html --config-path=./test/lighthouse/configs/full-config.js",
    "test:lighthouse:perf": "lighthouse http://localhost:8080 view --output-path=./test/lighthouse/reports/report.json --output json --config-path=./test/lighthouse/configs/perf-config.js",
    "dev": "quasar dev",
    "build": "quasar build",
    "build-spa": "quasar build -t mat && quasar build -t ios",
    "build-ssr-pwa": "yarn build-ssr && yarn build-pwa",
    "build-ssr": "quasar build -m ssr",
    "build-pwa": "quasar build -m pwa",
    "build-desktop": "quasar build --mode electron -t mat && quasar build --mode electron -t ios",
    "build-mobile": "yarn build-android && yarn build-ios",
    "build-android": "quasar build -m cordova -T android -t mat",
    "build-ios": "quasar build -m cordova -T ios -t ios"
  },
  "dependencies": {
    "axios": "^0.21.3"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "@babel/types": "^7.0.0",
    "@vue/test-utils": "^1.0.0-beta.23",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.1",
    "babel-jest": "^23.4.2",
    "chai": "^4.1.2",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.0",
    "devtron": "1.4.0",
    "electron": "^2.0.8",
    "electron-builder": "^20.28.4",
    "electron-debug": "1.5.0",
    "electron-devtools-installer": "2.2.4",
    "electron-installer-zip": "^0.1.2",
    "electron-packager": "^12.0.1",
    "eslint": "^4.18.2",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.3.0",
    "eslint-plugin-vue-a11y": "^0.0.26",
    "fse": "^4.0.1",
    "jest": "^23.4.2",
    "jest-serializer-vue": "^2.0.2",
    "jsdom": "^11.12.0",
    "jsdom-global": "^3.0.2",
    "lighthouse": "^3.0.3",
    "mocha": "^5.2.0",
    "node-fetch": "^2.2.0",
    "nodemon": "^1.18.3",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "quasar-cli": "^0.17.13",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "shelljs": "^0.8.2",
    "strip-ansi": "=3.0.1",
    "vue": "^2.5.17",
    "vue-jest": "^2.6.0",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.23.1"
  },
  "engines": {
    "node": ">= 8.9.0",
    "npm": ">= 5.6.0",
    "yarn": ">= 1.3.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
